{
  "name": "sf-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch ./src/i18n/config.js --watch ./src/i18n/locales --watch ./public/assets/icons --watch ./public/assets/images --ext ts,tsx,js,svg,json,jpeg,png --exec 'npm run images:convert && npm run i18n:type && npm run icons:type && NODE_ENV=development next dev'",
    "build": "npm run images:convert && npm run icons:type && npm run i18n:type && NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "git-hooks:init": "sh ./scripts/git-hooks:init.sh",
    "icons:type": "node ./scripts/icons:type.js",
    "images:convert": "sh ./scripts/images:convert.sh",
    "sdk:compile": "node ./scripts/sdk:compile.js",
    "translations:sort": "node ./scripts/translations:sort.js",
    "test:e2e": "playwright install && DEBUG=pw:webserver playwright test",
    "test:e2e:update-snapshots": "playwright install && DEBUG=pw:webserver playwright test --update-snapshots",
    "playwright:codegen": "playwright codegen localhost:3000",
    "playwright:show-report": "playwright show-report",
    "i18n:type": "node ./scripts/i18n:type.js",
    "test:jest": "jest --coverage",
    "test": "npm run test:jest && npm run test:e2e"
  },
  "dependencies": {
    "@sentry/nextjs": "7.53.1",
    "@tanstack/eslint-plugin-query": "4.29.9",
    "@tanstack/react-query": "4.29.12",
    "accept-language": "3.0.18",
    "ajv": "8.12.0",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "err-code": "3.0.1",
    "html-react-parser": "3.0.16",
    "i18next": "22.5.0",
    "i18next-resources-to-backend": "1.1.4",
    "immer": "9.0.19",
    "just-clone": "6.2.0",
    "lodash.merge": "4.6.2",
    "next": "13.4.3",
    "next-compose-plugins": "2.2.1",
    "next-i18next": "13.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "react-google-recaptcha": "2.1.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-hook-form": "7.45.0",
    "react-i18next": "12.3.1",
    "react-inlinesvg": "3.0.2",
    "react-select": "5.7.3",
    "react-tiny-popover": "7.2.4",
    "react-toastify": "9.1.3",
    "sdk": "github:geriko-dev/geriko-api-sdk",
    "server-only": "0.0.1",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@next/bundle-analyzer": "13.4.3",
    "@playwright/test": "1.33.0",
    "@soluzioni-futura/eslint-config-soluzioni-futura": "2.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/google.accounts": "0.0.7",
    "@types/jest": "29.5.2",
    "@types/json-schema": "7.0.11",
    "@types/lodash.merge": "4.6.7",
    "@types/node": "18.0.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-google-recaptcha": "2.1.5",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "autoprefixer": "10.4.14",
    "babel-jest": "29.5.0",
    "dotenv": "16.3.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.3",
    "eventsource-polyfill": "0.9.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "nodemon": "2.0.22",
    "openapi-typescript-codegen": "0.24.0",
    "postcss": "8.4.23",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "8.4.0",
    "tailwindcss": "3.3.2",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "^18.14.0",
    "npm": "^9.3.1"
  }
